[rpmfusion-nonfree-updates]
name=RPM Fusion for Fedora 31 - Nonfree - Updates
#baseurl=http://download1.rpmfusion.org/nonfree/fedora/updates/31/$basearch/
metalink=https://mirrors.rpmfusion.org/metalink?repo=nonfree-fedora-updates-released-31&arch=$basearch
enabled=1
enabled_metadata=1
type=rpm-md
gpgcheck=1
repo_gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-nonfree-fedora-31

